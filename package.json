    "react-native-native-env": "github:aksonov/react-native-native-env",
    "react-native-parsed-text": "0.0.16",
    "react-native-progress": "^3.2.1",
    "react-native-push-notification": "^2.2.1",
    "react-native-radio-button": "^1.0.2",
    "react-native-radio-buttons": "^0.13.0",
    "react-native-router-flux": "github:aksonov/react-native-router-flux#29090461469d4eb907072c63e9881f99205fab73",
    "react-native-stateless-form": "^0.3.0",
    "react-native-swipeable": "^0.4.0",
    "react-native-swiper": "1.5.12",
    "react-native-tab-view": "^0.0.66",
