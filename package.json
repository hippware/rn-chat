{
  "name": "tinyrobot",
  "version": "2.5.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:clean": "watchman watch-del-all && npm start -- --reset-cache",
    "xcode:clean": "rm -rf ios/DerivedData/* && rm -rf ios/build/*",
    "setup": "npm run xcode:clean && yarn && cd ios && pod install && cd ..",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone 7' --scheme tinyrobotStaging",
    "ios:prod": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone 7' --scheme tinyrobot",
    "test": "npm run lint && npm run tsc && npm run mocha && npm run jest:all",
    "tsc": "node node_modules/.bin/tsc",
    "testStaging": "env STAGING=true mocha ./test/* --require babel-core/register  --require babel-polyfill --require ./test/setup.js  --require mocha-steps --timeout 15000",
    "mocha": "mocha ./test/* --require babel-core/register  --require babel-polyfill  --require mocha-steps --timeout 10000",
    "jest:all": "jest --testPathPattern __tests__/*.test.js",
    "jest": "jest",
    "snapshot": "jest --testPathPattern=\"__tests__\" --updateSnapshot",
    "testBundle":
      "react-native bundle --bundle-output ./main.jsbundle --dev false --entry-file index.ios.js --platform ios && rm ./main.jsbundle ./main.jsbundle.meta",
    "lint": "tslint -p . -c tslint.json  'src/**/*.tsx'",
    "prettify": "prettier --write  'src/**/*.ts?(x)'",
    "codepush": "babel-node ./scripts/codepush.js",
    "version:bump": "babel-node ./scripts/version-increment.js",
    "deploy:stage": "babel-node ./scripts/deploy-stage.js",
    "bugsnag": "babel-node ./scripts/send-bugsnag-symbols.js",
    "detox": "detox build --configuration ios.sim.release && detox test --configuration ios.sim.release"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-clone-referenced-element|mobx|react-navigation|apsl-react-native-button)"
    ],
    "globalSetup": "./__tests__/utils/setupJest.js",
    "modulePaths": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*(test|spec)\\.(jsx?|tsx?))$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/tinyrobotStaging.app",
        "build": "xcodebuild -workspace ios/tinyRobot.xcworkspace -scheme tinyrobotStaging -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/tinyrobotStaging.app",
        "build": "xcodebuild -workspace ios/tinyRobot.xcworkspace -scheme tinyrobotStaging -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  },
  "dependencies": {
    "algoliasearch": "^3.20.4",
    "apsl-react-native-button": "^3.1.0",
    "assert": "^1.3.0",
    "autobind": "^1.0.3",
    "autobind-decorator": "^1.3.4",
    "base-64": "^0.1.0",
    "bugsnag-react-native": "^2.9.1",
    "cd": "^0.3.3",
    "libphonenumber-js": "^0.4.29",
    "lodash": "^4.17.4",
    "mobx": "3.3.1",
    "mobx-react": "4.3.5",
    "mobx-state-tree": "1.3.1",
    "moment": "^2.18.1",
    "mst-middlewares": "^1.3.1",
    "phoneformat.js": "github:hippware/phoneformat.js",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-native": "0.53.0",
    "react-native-actionsheet": "^2.2.2",
    "react-native-background-fetch": "^2.4.1",
    "react-native-background-geolocation": "^2.12.0-beta.2",
    "react-native-blur": "^3.2.2",
    "react-native-camera-kit": "github:aksonov/react-native-camera-kit#6fb593bed81547a68e039a77c5042f070e3a9fa1",
    "react-native-carrier-info": "github:hippware/react-native-carrier-info",
    "react-native-code-push": "5.1.3-beta",
    "react-native-country-picker-modal": "github:hippware/react-native-country-picker-modal",
    "react-native-cube-transition": "github:hippware/react-native-cube-transition#eb50153a468d81666426d03ced937a52cafc886c",
    "react-native-device-info": "^0.12.1",
    "react-native-firebase": "^3.3.1",
    "react-native-fs": "^2.8.5",
    "react-native-image-crop-picker": "^0.18.1",
    "react-native-keyboard-aware-scroll-view": "^0.5.0",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-maps": "^0.21.0",
    "react-native-message-bar": "^2.0.7",
    "react-native-mixpanel": "^0.0.16",
    "react-native-native-env": "github:aksonov/react-native-native-env#6e89ac8cd2dc26e4421b1d85a61529bd02643aa6",
    "react-native-parsed-text": "^0.0.20",
    "react-native-permissions": "^1.1.1",
    "react-native-push-notification": "^3.0.1",
    "react-native-router-flux": "github:aksonov/react-native-router-flux#37c6553c168b9fb6947a5e533df9bd37793f1f68",
    "react-native-swipeable": "^0.6.0",
    "react-native-swiper": "1.5.13",
    "react-native-tab-view": "^0.0.73",
    "react-native-xmpp": "github:hippware/react-native-xmpp#73deb74834989ae39a06fcaa0b413370b3f1cd23",
    "recompose": "^0.26.0",
    "remotedev": "^0.2.7",
    "stream-to-array": "^2.3.0",
    "validate.js": "github:hippware/validate.js#969b4fb274b0bb5234b8fe4199cc07dc1acb536e",
    "wocky-client": "github:hippware/wocky-client#2091",
    "xregexp": "^4.1.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/react": "^16.1.0",
    "@types/react-native": "^0.55.1",
    "appcenter-cli": "^1.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4",
    "babel-plugin-mobx-deep-action": "^1.5.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-remove-console": "^6.8.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "1.9.1",
    "chalk": "^1.1.3",
    "crypto": "^0.0.3",
    "detox": "^7.1.0",
    "estraverse-fb": "^1.3.1",
    "expect": "^1.13.0",
    "form-data": "^1.0.0-rc4",
    "jest": "^22.4",
    "mocha": "^2.3.4",
    "mocha-steps": "^0.0.1",
    "nock": "^3.3.0",
    "node-fetch": "^1.4.1",
    "prettier": "1.11.1",
    "promise": "^7.1.1",
    "react-native-mock": "^0.2.2",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "^16.0.0",
    "reactotron-mst": "^1.15.0",
    "reactotron-react-native": "^1.14.0",
    "readline-sync": "^1.4.7",
    "ts-jest": "^22.4.2",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1",
    "websocket": "^1.0.22",
    "xhr2": "^0.1.3",
    "xmlhttprequest": "^1.8.0"
  }
}