{
  "name": "Chat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "startdev": "babel-node index.js",
    "sign": "node keys/sign.js",
    "watch": "./node_modules/watchman/watchman state/model.scxml 'npm run gen'",
    "gen": "node node_modules/statem/src/parser.js state/model.scxml gen/state.js",
    "genold": "node node_modules/scxml/lib/compiler/scxml-to-scjson.js state/model.scxml > gen/model.json;node node_modules/scxml/lib/compiler/scjson-to-module.js --type commonjs gen/model.json > gen/model.js",
    "test": "mocha ./test/* --require react-native-mock/mock.js --require babel-core/register  --require babel-polyfill --require ./test/setup.js  --require mocha-steps --timeout 15000"
  },
  "browser": {
    "net": "react-native-tcp"
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
