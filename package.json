    "babel-runtime": "^6.26.0",
    "bugsnag-sourcemaps": "^1.0.4",
    "chalk": "^2.4.1",
    "denodeify": "^1.2.1",
    "expect": "^23.6.0",
    "form-data": "^1.0.0-rc4",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "node-fetch": "^2.1.2",
    "prettier": "1.11.1",
    "pretty-quick": "^1.8.0",
    "promise": "^7.1.1",
    "react-dom": "^16.6.0",
    "react-native-typescript-transformer": "^1.2.3",
