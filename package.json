{
  "name": "tinyrobot",
  "version": "3.9.2",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "xcode:clean": "rm -rf ios/DerivedData/* && rm -rf ios/build/*",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone 7' --scheme tinyrobotStaging",
    "test": "npm run testLocal && npm run mochaWocky",
    "testLocal": "npm run lint && npm run lintWocky && npm run tsc && npm run mocha && npm run jest:all",
    "tsc": "node node_modules/.bin/tsc -p tsconfig.build.json",
    "mocha": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha ./test/* --require ts-node/register --require tsconfig-paths/register --timeout 10000",
    "mochaWocky": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha ./third-party/wocky-client/test/* --require ts-node/register --require tsconfig-paths/register --require ./third-party/wocky-client/test/support/setup.js  --timeout 30000",
    "mochaWockyLocal": "WOCKY_LOCAL=true npm run mochaWocky -- --timeout 5000",
    "jest:all": "jest --testPathPattern __tests__/*.test.tsx",
    "jest": "jest",
    "lint": "tslint -p . -c tslint.json  'src/**/*.tsx'",
    "lintWocky": "tslint -p . -c tslint.json  'third-party/wocky-client/**/*.ts'",
    "prettify": "prettier --write  'src/**/*.ts?(x)'",
    "codepush": "bash ./scripts/codepush.sh",
    "version:bump": "babel-node ./scripts/version-increment.js",
    "bugsnag": "babel-node ./scripts/send-bugsnag-symbols.js",
    "detox": "detox build --configuration ios.sim.release && TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' detox test --configuration ios.sim.release",
    "precommit": "pretty-quick --staged",
    "fetchGraphQLSchema": "ts-node -O '{\"module\": \"commonjs\"}' scripts/fetchGraphQLInfo.ts",
    "syncWockyClient": "git subtree push --prefix=\"third-party/wocky-client\" https://github.com/hippware/wocky-client master"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./__tests__/utils/testSetup.ts",
      "./node_modules/appcenter/test/AppCenterMock.js",
      "./node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "./node_modules/appcenter-crashes/test/AppCenterCrashesMock.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-clone-referenced-element|mobx|react-navigation|apsl-react-native-button)"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/third-party/wocky-client/src"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*(test|spec)\\.(jsx?|tsx?))$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "wocky-client": "third-party/wocky-client/src"
    }
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/tinyrobotStaging.app",
        "build": "xcodebuild -workspace ios/tinyrobot.xcworkspace -scheme tinyrobotStaging -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/tinyrobotStaging.app",
        "build": "xcodebuild -workspace ios/tinyrobot.xcworkspace -scheme tinyrobotStaging -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "mocha",
    "runner-config": "e2e/detox-mocha.opts"
  },
  "dependencies": {
    "@absinthe/socket-apollo-link": "^0.1.11",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.1",
    "appcenter": "^1.6.0",
    "appcenter-analytics": "^1.6.0",
    "appcenter-crashes": "^1.6.0",
    "apsl-react-native-button": "^3.1.0",
    "assert": "^1.3.0",
    "autobind-decorator": "^1.3.4",
    "base-64": "^0.1.0",
    "bugsnag-react-native": "^2.9.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "libphonenumber-js": "^0.4.29",
    "lodash": "^4.17.4",
    "mobx": "^5.5.2",
    "mobx-react": "^5.0.0",
    "mobx-state-tree": "^3.7.1",
    "moment": "^2.18.1",
    "mst-middlewares": "^3.7.1",
    "phoneformat.js": "github:hippware/phoneformat.js",
    "prop-types": "^15.6.0",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-actionsheet": "^2.2.2",
    "react-native-background-geolocation": "^2.13.4",
    "react-native-blur": "^3.2.2",
    "react-native-camera-kit": "^6.2.6",
    "react-native-carrier-info": "github:hippware/react-native-carrier-info",
    "react-native-code-push": "^5.3.4",
    "react-native-country-picker-modal": "github:hippware/react-native-country-picker-modal",
    "react-native-device-info": "^0.12.1",
    "react-native-emoji-selector": "^0.1.6",
    "react-native-firebase": "^4.2.0",
    "react-native-fs": "^2.8.5",
    "react-native-image-crop-picker": "^0.21.2",
    "react-native-keyboard-aware-scroll-view": "^0.5.0",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-maps": "^0.21.0",
    "react-native-maps-super-cluster": "^1.4.0",
    "react-native-message-bar": "^2.0.7",
    "react-native-mixpanel": "^0.0.16",
    "react-native-native-env": "github:aksonov/react-native-native-env#6e89ac8cd2dc26e4421b1d85a61529bd02643aa6",
    "react-native-parsed-text": "^0.0.20",
    "react-native-permissions": "^1.1.1",
    "react-native-push-notification": "^3.0.1",
    "react-native-router-flux": "^4.0.3",
    "react-native-snap-carousel": "^3.7.1",
    "react-native-swipeable": "^0.6.0",
    "react-native-swiper": "1.5.13",
    "react-native-tab-view": "^0.0.73",
    "react-native-touch-through-view": "^0.1.4",
    "react-native-xmpp": "github:hippware/react-native-xmpp#73deb74834989ae39a06fcaa0b413370b3f1cd23",
    "react-navigation": "^2.x",
    "remotedev": "^0.2.7",
    "strophejs": "github:hippware/strophejs",
    "tcomb-form-native": "^0.6.11",
    "validate.js": "github:hippware/validate.js#969b4fb274b0bb5234b8fe4199cc07dc1acb536e",
    "xmldom": "^0.1.27",
    "xregexp": "^4.1.1"
  },
  "resolutions": {
    "zen-observable": "github:aksonov/zen-observable#62be6b655646c01ae3b1f43b2c6d13247cf06581",
    "@absinthe/socket": "github:aksonov/absinthe-socket#6b14cd8ecb18068a4bea95c80e75b23790926da4"
  },
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/graphql": "^0.12.7",
    "@types/jest": "^23.3.7",
    "@types/mocha": "2.2.43",
    "@types/node": "^9.3.0",
    "@types/node-fetch": "^1.6.7",
    "@types/phoenix": "^0.0.4",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.16",
    "@types/react-navigation": "^2.0.12",
    "@types/react-test-renderer": "^16.0.3",
    "@types/strophe": "1.2.27",
    "@types/xmldom": "^0.1.29",
    "appcenter-cli": "^1.0.17",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-remove-console": "^6.8.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "bugsnag-sourcemaps": "^1.0.3",
    "chai": "4.1.2",
    "chalk": "^1.1.3",
    "denodeify": "^1.2.1",
    "expect": "^1.13.0",
    "form-data": "^1.0.0-rc4",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "mocha": "^2.3.4",
    "mocha-steps": "^0.0.1",
    "node-fetch": "^2.1.2",
    "prettier": "1.11.1",
    "pretty-quick": "^1.4.1",
    "promise": "^7.1.1",
    "react-native-mock": "^0.3.1",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "^16.4.0",
    "readline-sync": "^1.4.7",
    "ts-jest": "^23.10.4",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.6.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.6.0",
