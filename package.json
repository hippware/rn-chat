{
  "name": "tinyrobot",
  "version": "4.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "xcode:clean": "rm -rf ios/DerivedData/* && rm -rf ios/build/*",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone 7' --scheme tinyrobotStaging",
    "test": "npm run testLocal && npm run jestWockyCI",
    "testLocal": "npm run lint && npm run lintWocky && npm run tsc:all && npm run jestCI",
    "tsc": "node node_modules/.bin/tsc -p tsconfig.build.json",
    "tsc:all": "node node_modules/.bin/tsc -p tsconfig.json",
    "jestWocky": "jest --config jest.wocky.config.json",
    "jestWockyCI": "jest --config jest.wocky.config.json --no-cache --i --detectOpenHandles --forceExit",
    "mochaWockyLocal": "WOCKY_LOCAL=true npm run mochaWocky -- --timeout 5000",
    "jest": "jest --config jest.config.json",
    "jestCI": "jest --config jest.config.json --verbose --runInBand",
    "lint": "tslint -p . -c tslint.json  'src/**/*.ts' 'src/**/*.tsx'",
    "lintWocky": "tslint -p . -c tslint.json  'third-party/wocky-client/src/**/*.ts'",
    "prettify": "prettier --write  'src/**/*.ts?(x)'",
    "codepush": "bash ./scripts/codepush.sh",
    "version:bump": "babel-node ./scripts/version-increment.js",
    "bugsnag": "babel-node ./scripts/send-bugsnag-symbols.js",
    "detox": "detox build --configuration ios.sim.release && TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' detox test --configuration ios.sim.release",
    "fetchGraphQLSchema": "ts-node -O '{\"module\": \"commonjs\"}' scripts/fetchGraphQLInfo.ts",
    "syncWockyClient": "git subtree push --prefix=\"third-party/wocky-client\" https://github.com/hippware/wocky-client master",
    "findUnusedExports": "./node_modules/.bin/ts-unused-exports ./tsconfig.json",
    "storybook": "storybook start"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./__tests__/utils/testSetup.ts",
      "./node_modules/appcenter/test/AppCenterMock.js",
      "./third-party/wocky-client/test/support/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-clone-referenced-element|mobx|react-navigation|apsl-react-native-button)"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/third-party/wocky-client/src"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "((/__tests__/.*(test|spec)\\.(jsx?|tsx?))|(third-party/wocky-client/(test/.*(test)\\.ts)))$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "wocky-client": "third-party/wocky-client/src"
    }
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/tinyrobotStaging.app",
        "build": "xcodebuild -workspace ios/tinyrobot.xcworkspace -scheme tinyrobotStaging -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/tinyrobotStaging.app",
        "build": "xcodebuild -workspace ios/tinyrobot.xcworkspace -scheme tinyrobotStaging -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "mocha",
    "runner-config": "e2e/detox-mocha.opts"
  },
  "dependencies": {
    "@absinthe/socket-apollo-link": "^0.2.0",
    "apollo-cache-inmemory": "^1.3.8",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.3",
    "appcenter": "^1.9.0",
    "assert": "^1.3.0",
    "autobind-decorator": "^2.1.0",
    "base-64": "^0.1.0",
    "bugsnag-react-native": "^2.9.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "jsrsasign": "^8.0.12",
    "libphonenumber-js": "^1.6.4",
    "lodash": "^4.17.11",
    "mobx": "5.5.2",
    "mobx-react": "5.3.6",
    "mobx-state-tree": "3.7.1",
    "moment": "^2.18.1",
    "mst-middlewares": "^3.7.1",
    "phoneformat.js": "github:hippware/phoneformat.js",
    "prop-types": "^15.6.0",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-actionsheet": "^2.2.2",
    "react-native-background-geolocation": "^2.14.1",
    "react-native-blur": "^3.2.2",
    "react-native-camera-kit": "^6.3.0",
    "react-native-code-push": "^5.3.4",
    "react-native-country-picker-modal": "github:hippware/react-native-country-picker-modal",
    "react-native-device-info": "^0.12.1",
    "react-native-emoji-selector": "^0.1.6",
    "react-native-firebase": "^5.0.0",
    "react-native-fs": "^2.8.5",
    "react-native-image-crop-picker": "^0.21.3",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-linear-gradient": "^2.4.3",
    "react-native-maps": "^0.22.0",
    "react-native-message-bar": "^2.0.7",
    "react-native-mixpanel": "^1.1.1",
    "react-native-native-env": "github:aksonov/react-native-native-env#6e89ac8cd2dc26e4421b1d85a61529bd02643aa6",
    "react-native-permissions": "^1.1.1",
    "react-native-push-notification": "^3.1.2",
    "react-native-router-flux": "^4.0.3",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-swipeable": "^0.6.0",
    "react-native-swiper": "1.5.13",
    "react-native-touch-through-view": "^0.1.4",
    "react-navigation": "^2.x",
    "remotedev": "^0.2.7",
    "tcomb-form-native": "^0.6.20",
    "validate.js": "github:hippware/validate.js#969b4fb274b0bb5234b8fe4199cc07dc1acb536e",
    "xregexp": "^4.1.1"
  },
  "resolutions": {		
    "@jumpn/utils-graphql": "github:hippware/utils-graphql#eff846b"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@storybook/addon-actions": "^4.0.7",
    "@storybook/addon-links": "^4.0.7",
    "@storybook/addons": "^4.0.7",
    "@storybook/react-native": "^4.0.7",
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/node": "^9.3.0",
    "@types/node-fetch": "^2.1.2",
    "@types/phoenix": "^0.0.4",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.16",
    "@types/react-navigation": "^2.13.0",
    "@types/react-test-renderer": "^16.0.3",
    "@types/strophe": "1.2.27",
    "@types/xmldom": "^0.1.29",
    "appcenter-cli": "^1.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-remove-console": "^6.8.4",
    "babel-plugin-transform-runtime": "^6.23.0",
