{
  "name": "tinyrobot",
  "version": "1.54.0",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:clean": "watchman watch-del-all && npm start -- --reset-cache",
    "xcode:clean": "rm -rf ios/DerivedData/* && rm -rf ios/build/*",
    "setup": "npm run xcode:clean && yarn && cd ios && pod install && cd ..",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone 7' --scheme tinyrobotStaging",
    "ios:prod": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone 7' --scheme tinyrobot",
    "test": "mocha ./test/* --require babel-core/register  --require babel-polyfill --require ./test/setup.js  --require mocha-steps --timeout 15000",
    "testStaging": "env STAGING=true mocha ./test/* --require babel-core/register  --require babel-polyfill --require ./test/setup.js  --require mocha-steps --timeout 15000",
    "testNew": "jest",
    "prettify": "prettier-eslint --write \"src/**/*.js\"",
    "codepush": "babel-node ./scripts/codepush.js",
    "version:bump": "babel-node ./scripts/version-increment.js && git add . && babel-node ./scripts/version-commit.js",
    "version:tag": "babel-node ./scripts/version-tag.js && git push origin --tags",
    "version:push": "git push origin `git rev-parse --abbrev-ref HEAD` && git push origin `git rev-parse --abbrev-ref HEAD`:deploy-stage -f",
    "version:promote": "babel-node ./scripts/version-promote.js",
    "deploy:stage": "npm run version:bump && npm run version:tag && npm run version:push && TARGET_ENV=stage npm run version:promote",
    "promote:prod": "TARGET_ENV=prod npm run version:promote"
  },
  "dependencies": {
    "algoliasearch": "^3.20.4",
    "apsl-react-native-button": "^3.0.2",
    "assert": "^1.3.0",
    "autobind": "^1.0.3",
    "autobind-decorator": "^1.3.4",
    "base-64": "^0.1.0",
    "bugsnag-react-native": "^2.0.1",
    "cd": "^0.3.3",
    "kefir": "^3.7.0",
    "libphonenumber-js": "^0.4.29",
    "lodash": "^4.17.4",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "moment": "^2.18.1",
    "phoneformat.js": "github:hippware/phoneformat.js",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.2",
    "react-native-actionsheet": "^2.2.2",
    "react-native-background-geolocation": "^2.9.3",
    "react-native-blur": "^3.0.0-alpha",
    "react-native-button": "^1.7.1",
    "react-native-camera-kit": "github:aksonov/react-native-camera-kit#21a3853e36ff233565ecc799e5c0d599631b6a1e",
    "react-native-carrier-info": "github:hippware/react-native-carrier-info",
    "react-native-code-push": "5.0.0-beta",
    "react-native-country-picker-modal": "^0.3.0",
    "react-native-cube-transition": "github:hippware/react-native-cube-transition#1e473754642763b9d8396c9bd463ed454c447dd7",
    "react-native-device-info": "^0.9.7",
    "react-native-firebase": "^3.0.0",
    "react-native-fs": "^2.1.0-rc.1",
    "react-native-image-crop-picker": "^0.14.4",
    "react-native-invertible-flat-list": "^1.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-linear-gradient": "^2.0.0",
