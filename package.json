    "jest": "18.1.0",
    "mocha": "^2.3.4",
    "mocha-steps": "^0.0.1",
    "nock": "^3.3.0",
    "node-fetch": "^1.4.1",
    "prettier-eslint": "^8.1.0",
    "prettier-eslint-cli": "^4.3.2",
    "promise": "^7.1.1",
    "react-native-mock": "^0.2.2",
    "react-test-renderer": "15.4.2",
    "readline-sync": "^1.4.7",
    "websocket": "^1.0.22",
    "xhr2": "^0.1.3",
    "xmlhttprequest": "^1.8.0"
  },
  "jest": {
    "preset": "react-native"
  }
