{
  "name": "wocky-client",
  "version": "1.0.0",
  "description": "XMPP wocky client",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js src --quiet",
    "test": "npm run lint && npm run mocha",
    "mocha": "mocha ./test/* --require babel-core/register  --require ./test/support/setup.js --require babel-polyfill --timeout 60000",
    "prettify": "prettier-eslint --write  \"@(src|test|utils|__tests__|__mocks__|scripts)/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hippware/wocky-client.git"
  },
  "keywords": [
    "wocky"
  ],
  "author": "Pavlo Aksonov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hippware/wocky-client/issues"
  },
  "homepage": "https://github.com/hippware/wocky-client#readme",
  "dependencies": {
    "assert": "^1.4.1",
    "mobx": "^3.3.1",
    "mobx-state-tree": "^1.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^2.3.4",
    "prettier-eslint": "^8.7.3",
    "xmlhttprequest": "^1.8.0"
