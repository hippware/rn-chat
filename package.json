{
  "name": "tinyrobot",
  "version": "1.33.1",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "mocha ./test/* --require babel-core/register  --require babel-polyfill --require ./test/setup.js  --require mocha-steps --timeout 15000",
    "testStaging": "env STAGING=true mocha ./test/* --require babel-core/register  --require babel-polyfill --require ./test/setup.js  --require mocha-steps --timeout 15000",
    "testNew": "jest --watch",
    "gen": "node node_modules/statem/src/parser.js state/model.scxml gen/state.js",
    "prettify": "prettier-eslint --write \"src/**/*.js\"",
    "codepush": "babel-node ./scripts/codepush.js",
    "version:bump": "babel-node ./scripts/version-increment.js && git add . && babel-node ./scripts/version-commit.js",
    "version:tag": "babel-node ./scripts/version-tag.js && git push origin --tags",
    "version:push": "git push origin `git rev-parse --abbrev-ref HEAD` && git push origin `git rev-parse --abbrev-ref HEAD`:deploy-stage -f",
    "version:promote": "babel-node ./scripts/version-promote.js",
    "deploy:stage": "npm run version:bump && npm run version:tag && npm run version:push && TARGET_ENV=stage npm run version:promote",
    "promote:prod": "TARGET_ENV=prod npm run version:promote"
  },
  "dependencies": {
    "algoliasearch": "^3.20.4",
    "apsl-react-native-button": "^3.0.2",
    "assert": "^1.3.0",
    "autobind": "^1.0.3",
    "autobind-decorator": "^1.3.4",
    "bugsnag-react-native": "^2.0.1",
    "cd": "^0.3.3",
    "kefir": "^3.7.0",
    "mobx": "^3.0.0",
    "mobx-react": "^4.1.3",
    "moment": "^2.18.1",
    "phoneformat.js": "github:hippware/phoneformat.js",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-background-geolocation": "^2.8.3",
    "react-native-blur": "^3.0.0-alpha",
    "react-native-button": "^1.7.1",
    "react-native-camera": "^1.0.0-alpha1",
    "react-native-carrier-info": "github:hippware/react-native-carrier-info",
    "react-native-code-push": "2.0.3-beta",
    "react-native-device-info": "^0.9.7",
    "react-native-fabric-digits": "^1.0.26",
    "react-native-fs": "^2.1.0-rc.1",
    "react-native-image-crop-picker": "^0.13.1",
    "react-native-infinite-scroll-view": "^0.4.2",
    "react-native-invertible-scroll-view": "^1.0.0",
    "react-native-ios-controllers": "^2.4.0",
    "react-native-keyboard-aware-scroll-view": "^0.2.9",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-mapbox-gl": "github:hippware/react-native-mapbox-gl#5.2.1",
    "react-native-message-bar": "^1.6.0",
    "react-native-mixpanel": "github:hippware/react-native-mixpanel",
    "react-native-native-env": "github:aksonov/react-native-native-env",
    "react-native-parallax-scroll-view": "^0.19.0",
    "react-native-parsed-text": "0.0.16",
    "react-native-progress": "^3.2.1",
    "react-native-push-notification": "^2.2.1",
    "react-native-radio-button": "^1.0.2",
    "react-native-radio-buttons": "^0.13.0",
    "react-native-router-native": "^0.2.2",
    "react-native-stateless-form": "^0.3.0",
    "react-native-swipeable": "^0.4.0",
    "react-native-swiper": "^1.5.4",
    "react-native-tab-view": "^0.0.66",
    "react-native-tabs": "^1.0.9",
    "react-native-xmpp": "github:hippware/react-native-xmpp#0.5.7",
    "recompose": "^0.23.1",
    "serializr": "^1.1.9",
    "statem": "^0.6.0",
    "stream-to-array": "^2.3.0",
    "strophejs": "github:hippware/strophejs",
    "suncalc": "^1.8.0",
    "validate.js": "^0.11.1",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^2.0.0",
    "chalk": "^1.1.3",
    "crypto": "^0.0.3",
    "denodeify": "^1.2.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "estraverse-fb": "^1.3.1",
    "expect": "^1.13.0",
    "flow-bin": "0.36.0",
