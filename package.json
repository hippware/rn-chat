{
  "name": "tinyrobot",
  "version": "1.65.3",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:clean": "watchman watch-del-all && npm start -- --reset-cache",
    "xcode:clean": "rm -rf ios/DerivedData/* && rm -rf ios/build/*",
    "setup": "npm run xcode:clean && yarn && cd ios && pod install && cd ..",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone 7' --scheme tinyrobotStaging",
    "ios:prod": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone 7' --scheme tinyrobot",
    "test": "npm run mocha && npm run jest:all && npm run testBundle",
    "testStaging": "env STAGING=true mocha ./test/* --require babel-core/register  --require babel-polyfill --require ./test/setup.js  --require mocha-steps --timeout 15000",
    "mocha": "mocha ./test/* --require babel-core/register  --require babel-polyfill --require ./test/setup.js  --require mocha-steps --timeout 60000",
    "mocha:store": "mocha ./test/store/*.test.js --require babel-core/register  --require babel-polyfill --require ./test/setup.js --timeout 1000",
    "jest:all": "jest --testPathPattern __tests__/*.test.js",
    "jest": "jest",
    "snapshot": "jest --testPathPattern=\"__tests__\" --updateSnapshot",
    "testBundle": "react-native bundle --bundle-output ./main.jsbundle --dev false --entry-file index.ios.js --platform ios && rm ./main.jsbundle ./main.jsbundle.meta",
    "lint": "eslint src test __tests__ __mocks__ scripts utils index.android.js index.ios.js --quiet",
    "prettify": "prettier-eslint --write  \"@(src|test|utils|__tests__|__mocks__|scripts)/**/*.js\"",
    "codepush": "babel-node ./scripts/codepush.js",
    "version:bump": "babel-node ./scripts/version-increment.js && git add . && babel-node ./scripts/version-commit.js",
    "version:tag": "babel-node ./scripts/version-tag.js && git push origin --tags",
    "version:push": "git push origin `git rev-parse --abbrev-ref HEAD` && git push origin `git rev-parse --abbrev-ref HEAD`:deploy-stage -f",
    "version:promote": "babel-node ./scripts/version-promote.js",
    "deploy:stage": "npm run version:bump && npm run version:tag && npm run version:push && TARGET_ENV=stage npm run version:promote",
    "promote:prod": "TARGET_ENV=prod npm run version:promote"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-clone-referenced-element|mobx|react-navigation|apsl-react-native-button)"
    ],
    "globalSetup": "./__tests__/setupJest.js"
  },
  "dependencies": {
    "algoliasearch": "^3.20.4",
    "apsl-react-native-button": "^3.1.0",
    "assert": "^1.3.0",
    "autobind": "^1.0.3",
    "autobind-decorator": "^1.3.4",
    "base-64": "^0.1.0",
    "bugsnag-react-native": "^2.6.1",
    "cd": "^0.3.3",
    "libphonenumber-js": "^0.4.29",
    "lodash": "^4.17.4",
    "mobx": "3.3.1",
    "mobx-react": "4.3.5",
    "mobx-state-tree": "1.3.1",
    "moment": "^2.18.1",
    "mst-middlewares": "^1.3.1",
    "phoneformat.js": "github:hippware/phoneformat.js",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-native": "^0.50.3",
    "react-native-actionsheet": "^2.2.2",
    "react-native-background-geolocation": "^2.9.3",
    "react-native-blur": "^3.2.2",
    "react-native-camera-kit": "github:aksonov/react-native-camera-kit#6fb593bed81547a68e039a77c5042f070e3a9fa1",
    "react-native-carrier-info": "github:hippware/react-native-carrier-info",
    "react-native-code-push": "5.1.3-beta",
    "react-native-country-picker-modal": "github:hippware/react-native-country-picker-modal",
    "react-native-cube-transition": "github:hippware/react-native-cube-transition#eb50153a468d81666426d03ced937a52cafc886c",
    "react-native-device-info": "^0.12.1",
    "react-native-firebase": "^3.1.1",
    "react-native-fs": "^2.8.5",
    "react-native-image-crop-picker": "^0.18.1",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-maps": "github:hippware/react-native-maps#ed822d90b6b006008a739b2b5a8574e01732c6a7",
    "react-native-message-bar": "^2.0.7",
    "react-native-mixpanel": "^0.0.16",
    "react-native-native-env": "github:aksonov/react-native-native-env#6e89ac8cd2dc26e4421b1d85a61529bd02643aa6",
    "react-native-parsed-text": "^0.0.20",
    "react-native-push-notification": "^3.0.1",
    "react-native-router-flux": "github:aksonov/react-native-router-flux#37c6553c168b9fb6947a5e533df9bd37793f1f68",
    "react-native-swipeable": "^0.6.0",
    "react-native-swiper": "1.5.12",
    "react-native-tab-view": "^0.0.73",
    "react-native-xmpp": "1.0.0-beta.2",
    "recompose": "^0.26.0",
    "remotedev": "^0.2.7",
    "stream-to-array": "^2.3.0",
    "validate.js": "^0.11.1",
    "wocky-client": "github:hippware/wocky-client#2e71f2f1855008d1bdfe33a39936345642c001e8"
  },
  "devDependencies": {
    "appcenter-cli": "^1.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-mobx-deep-action": "^1.5.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-remove-console": "^6.8.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "1.9.1",
    "chalk": "^1.1.3",
    "crypto": "^0.0.3",
    "denodeify": "^1.2.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "estraverse-fb": "^1.3.1",
    "expect": "^1.13.0",
    "flow-bin": "^0.56.0",
    "form-data": "^1.0.0-rc4",
    "jest": "^22.1.4",
    "mocha": "^2.3.4",
    "mocha-steps": "^0.0.1",
    "nock": "^3.3.0",
    "node-fetch": "^1.4.1",
    "prettier-eslint": "^8.7.4",
    "prettier-eslint-cli": "^4.7.0",
    "promise": "^7.1.1",
    "react-native-mock": "^0.2.2",
    "react-test-renderer": "^16.0.0",
    "reactotron-mst": "^1.15.0",
    "reactotron-react-native": "^1.14.0",
    "readline-sync": "^1.4.7",
    "websocket": "^1.0.22",
    "xhr2": "^0.1.3",
    "xmlhttprequest": "^1.8.0"
  }
}
